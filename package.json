{
  "name": "picture-library",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justin-barfield/picture-library.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.6.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "bugs": {
    "url": "https://github.com/justin-barfield/picture-library/issues"
  },
  "homepage": "https://github.com/justin-barfield/picture-library#readme",
  "dependencies": {
    "aws-sdk": "^2.567.0",
    "@deckdeckgo/qrcode": "^1.0.0-rc.1-1",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.3",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "model": "^6.0.1",
    "mongoose": "^5.7.6",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql2": "^1.7.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "qrcode.react": "^0.9.3",
    "react-art-svg-renderer": "0.0.1-rc.8",
    "react-dropzone": "^10.1.10",
    "react-image-crop": "^8.4.0",
    "react-qr-code": "^0.1.3",
    "react-qr-reader": "^2.2.1",
    "react-qr-svg": "^2.2.1",
    "sequelize": "^5.21.1"
  }
}
